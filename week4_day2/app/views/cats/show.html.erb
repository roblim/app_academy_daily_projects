<h1 align="center">Here's your cat! Enjoy!</h1>
<table width="60%" frame="border", rules="all", align="center">
  <tr>
    <th>Attributes</th>
    <th>Values</th>
  </tr>
  <tr>
    <td>Name:</td>
    <td><%= @cat.name %></td>
  </tr>
  <tr>
    <td> Birthday: </td>
    <td><%= @cat.birth_date %></td>
  </tr>
  <tr>
    <td>Color: </td>
    <td><%= @cat.color %></td>
  </tr>
  <tr>
    <td>Sex: </td>
    <td><%= @cat.sex %></td>
  </tr>
  <tr>
    <td> Description: </td>
    <td> <%= @cat.description %> </td>
  </tr>
</table>
